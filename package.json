  "scripts": {
    "postinstall": "tsc -p .",
    "lint": "tslint './**/*.ts' -c tslint.json -e './node_modules/**/*.ts'",
    "build": "tsc -p .",
    "test": "./petest-init-database.sh && jest --runInBand --forceExit --detectOpenHandles",
    "test:basic": "jest --runInBand --forceExit --detectOpenHandles --config=jest-basic.config.js",
    "test:ioc": "jest --runInBand --forceExit --detectOpenHandles --config=jest-ioc.config.js",
    "test:server": "jest --runInBand --forceExit --detectOpenHandles --config=jest-server.config.js",
    "test:data": "./pretest-init-database.sh && jest --runInBand --forceExit --detectOpenHandles --config=jest-data.config.js",
    "lint & test": "yarn run lint & yarn run test",
    "dev:basic@mac": "node $NODE_DEBUG_OPTION test-services/Bootstrap.js basic dev",
    "dev:basic@win": "node %NODE_DEBUG_OPTION% test-services/Bootstrap.js basic dev"
  },
