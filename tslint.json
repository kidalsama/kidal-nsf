{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "ordered-imports": false,
    "variable-name": {
      "options": [
        "allow-leading-underscore"
      ]
    },
    "no-console": {
      "severity": "warning"
    },
    "max-classes-per-file": true,
    "semicolon": false
  },
  "rulesDirectory": []
}
